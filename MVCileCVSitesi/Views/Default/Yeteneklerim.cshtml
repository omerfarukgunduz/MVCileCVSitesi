@using MVCileCVSitesi.Models.Entity
@model List<Tbl_Yeteneklerim>

<div class="resume-section-content">
    <h2 class="mb-5">Yeteneklerim</h2>
    <div class="subheading mb-3">Programlama Dilleri ve Araçlar</div>
    <ul class="list-inline dev-icons">
        <li class="list-inline-item"><i class="fab fa-html5"></i></li>
        <li class="list-inline-item"><i class="fab fa-css3-alt"></i></li>
        <li class="list-inline-item"><i class="fab fa-js"></i></li>
        <li class="list-inline-item"><i class="fab fa-bootstrap"></i></li>

    </ul>
    <div class="subheading mb-3">Beceriler</div>
    <ul class="fa-ul mb-0">
        @foreach(var x in Model)
        { 
        <li>
            <span class="fa-li"><i class="fas fa-check"></i></span>
            @x.Yetenek
        </li>
        }
    </ul>
</div>
